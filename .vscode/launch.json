    "version": "0.2.0",
    "configurations": [
        {
            "name": "openapi_base",
            "cwd": "packages/openapi_base",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "openapi_code_builder",
            "cwd": "packages/openapi_code_builder",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "openapi_generator_flutter",
            "cwd": "packages/openapi_generator_flutter",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "openapi_generator_flutter (profile mode)",
            "cwd": "packages/openapi_generator_flutter",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "openapi_generator_flutter (release mode)",
            "cwd": "packages/openapi_generator_flutter",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "example",
            "cwd": "packages/openapi_code_builder/example",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Debug Example",
            "request": "launch",
            "program": "${workspaceRoot}/packages/openapi_code_builder/example/.dart_tool/build/entrypoint/build.dart",
            "type": "dart",
            "args": ["build", "--delete-conflicting-outputs"]
        }     
    ]
}